var x, y, y_original, z;

proc main()

  writestr("x: ");
  read(x);

  writestr("y: ");
  read(y_original);

  writestr("z: ");
  read(z);

  while (x <= z)
    y = y_original;
    while (y <= z)
      writestr("y: ");
      write(y);
      writestr(", x: ");
      write(x); line;
      y = y + 1;
    endwhile;
    x = x + 1;
  endwhile;

endproc